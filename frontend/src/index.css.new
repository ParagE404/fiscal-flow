}

/* Change indicator animations */
.change-indicator {
  position: relative;
  overflow: hidden;
}

.change-indicator::before {
  content: "";
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(
    90deg,
    transparent 0%,
    hsl(var(--primary) / 0.1) 50%,
    transparent 100%
  );
  transition: left var(--duration-500) var(--ease-out);
}

.financial-data-card:hover .change-indicator::before {
  left: 100%;
}

