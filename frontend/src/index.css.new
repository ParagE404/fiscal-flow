  /* Text alignment utilities */

  /* Font feature utilities */

  /* GPU-Accelerated Transition Classes */
  .transition-fast {
    transition-duration: var(--duration-150);
    transition-timing-function: var(--ease-out);
    transform: translateZ(0);
    will-change: transform, opacity;
  }

  .transition-normal {
    transition-duration: var(--duration-300);
    transition-timing-function: var(--ease-in-out);
    transform: translateZ(0);
    will-change: transform, opacity;
  }

  .transition-slow {
    transition-duration: var(--duration-500);
    transition-timing-function: var(--ease-smooth);
    transform: translateZ(0);
    will-change: transform, opacity;
  }

  .transition-bounce {
    transition-duration: var(--duration-300);
    transition-timing-function: var(--ease-bounce);
    transform: translateZ(0);
    will-change: transform, opacity;
  }

  /* GPU-Accelerated Modern Interaction States */
  .interactive {
    transition: all var(--duration-200) var(--ease-out);
    cursor: pointer;
    transform: translateZ(0);
    will-change: transform;
    backface-visibility: hidden;
  }

  .interactive:hover {
    transform: translateY(-1px) translateZ(0);
  }

  .interactive:active {
    transform: translateY(0) translateZ(0);
    transition-duration: var(--duration-75);
  }

  /* GPU-Accelerated Card Hover Effects */
  .card-hover {
    transition: all var(--duration-300) var(--ease-out);
    transform: translateZ(0);
    will-change: transform, box-shadow;
    backface-visibility: hidden;
  }

  .card-hover:hover {
    transform: translateY(-2px) translateZ(0);
    box-shadow: var(--shadow-lg);
  }

  /* GPU-Accelerated Button Animations */
  .btn-animate {
    transition: all var(--duration-200) var(--ease-out);
    position: relative;
    overflow: hidden;
    transform: translateZ(0);
    will-change: transform, box-shadow;
    backface-visibility: hidden;
  }

  .btn-animate:hover {
    transform: translateY(-1px) translateZ(0);
    box-shadow: var(--shadow-md);
  }

  .btn-animate:active {
    transform: translateY(0) translateZ(0);
    box-shadow: var(--shadow-sm);
    transition-duration: var(--duration-75);
  }

  /* Enhanced Ripple Effect */
  .ripple {
    position: relative;
    overflow: hidden;
  }

  .ripple::before {
    content: "";
    position: absolute;
    top: 50%;
    left: 50%;
    width: 0;
    height: 0;
    border-radius: 50%;
    background: rgba(255, 255, 255, 0.3);
    transform: translate(-50%, -50%);
    transition: width var(--duration-300) var(--ease-out),
      height var(--duration-300) var(--ease-out);
  }

  .ripple:active::before {
    width: 300px;
    height: 300px;
  }

  /* GPU-Accelerated Modern Button Animations */
  .btn-modern {
    position: relative;
    overflow: hidden;
    transition: all var(--duration-200) var(--ease-out);
    transform-origin: center;
    transform: translateZ(0);
    will-change: transform, box-shadow;
    backface-visibility: hidden;
  }

  .btn-modern:hover {
    transform: translateY(-2px) scale(1.02) translateZ(0);
    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.15), 0 4px 8px rgba(0, 0, 0, 0.1);
  }

  .btn-modern:active {
    transform: translateY(0) scale(0.98) translateZ(0);
    transition-duration: var(--duration-75);
  }

  .btn-modern:focus-visible {
    outline: none;
    box-shadow: 0 0 0 3px hsl(var(--ring) / 0.5), 0 8px 16px rgba(0, 0, 0, 0.15);
  }

  /* Gradient Button Hover Effects */
  .gradient-primary:hover {
    background: linear-gradient(
      135deg,
      hsl(var(--primary-blue-600)) 0%,
      hsl(var(--primary-blue-700)) 100%
    );
  }

  .gradient-success:hover {
    background: linear-gradient(
      135deg,
      hsl(var(--primary-green-600)) 0%,
      hsl(var(--primary-green-700)) 100%
    );
  }

  .gradient-purple:hover {
    background: linear-gradient(
      135deg,
      hsl(var(--primary-purple-600)) 0%,
      hsl(var(--primary-purple-700)) 100%
    );
  }

  .gradient-orange:hover {
    background: linear-gradient(
      135deg,
      hsl(var(--accent-orange-600)) 0%,
      hsl(var(--accent-orange-700)) 100%
    );
  }

  .gradient-accent:hover {
    background: linear-gradient(
      135deg,
      hsl(var(--accent-teal-600)) 0%,
      hsl(var(--accent-teal-700)) 100%
    );
  }

  .gradient-pink:hover {
    background: linear-gradient(
      135deg,
      hsl(var(--accent-pink-600)) 0%,
      hsl(var(--accent-pink-700)) 100%
    );
  }

  .gradient-warning:hover {
    background: linear-gradient(
      135deg,
      hsl(var(--warning-500)) 0%,
      hsl(var(--warning-600)) 100%
    );
  }

  .gradient-error:hover {
    background: linear-gradient(
      135deg,
      hsl(var(--error-500)) 0%,
      hsl(var(--error-600)) 100%
    );
  }

  /* Ghost Button Fill Animations */
  .ghost-fill {
    position: relative;
    overflow: hidden;
  }

  .ghost-fill::before {
    content: "";
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(
      135deg,
      hsl(var(--primary)) 0%,
      hsl(var(--primary) / 0.9) 100%
    );
    transition: left var(--duration-300) var(--ease-out);
    z-index: -1;
  }

  .ghost-fill:hover::before {
    left: 0;
  }

  .ghost-fill:hover {
    color: hsl(var(--primary-foreground));
  }

  /* Icon Button Enhancements */

  .icon-btn-ripple::after {
    content: "";
    position: absolute;
    top: 50%;
    left: 50%;
    width: 0;
    height: 0;
    border-radius: 50%;
    background: rgba(255, 255, 255, 0.4);
    transform: translate(-50%, -50%);
    transition: width var(--duration-200) var(--ease-out),
      height var(--duration-200) var(--ease-out);
  }

  .icon-btn-ripple:active::after {
    width: 120%;
    height: 120%;
  }

  /* Floating Action Button Enhancements */

  /* Fade Animations */

  .fade-in-up {
    animation: fadeInUp var(--duration-500) var(--ease-out);
  }

  /* Error State Animations */
  .animate-fade-in {
    animation: fadeIn var(--duration-300) var(--ease-out);
  }

  .animate-fade-in-up {
    animation: fadeInUp var(--duration-400) var(--ease-out);
  }

  /* Scale Animations */

  /* Slide Animations */

  /* Loading Animations */

